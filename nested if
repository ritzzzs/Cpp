#include<iostream>
using namespace std;
int main(){
    int age=5;
    if(age>2){
        if(age>=5){
            cout<<"your age is equal to or greater than 5";
        }
        else{
        cout<<"Your age is not equal to greater than 5";
        }
    }
    else {
        if(age ==10){
            cout<<"your age is equal to 10";
        }
        else{
            cout<<"your age is not equal to 10";
        }
    }
    return 0;
}
